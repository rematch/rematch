{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "interface-name": false,
        "no-submodule-imports": false,
        "no-console": [
            true,
            "log",
            "warn"
        ],
        "member-ordering": false
    },
    "rulesDirectory": []
}