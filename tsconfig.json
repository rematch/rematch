{
  "compilerOptions": {
      "allowJs": true,
      "sourceMap": true,
      "target": "es5",
      "lib": ["es2017", "dom"],
      "moduleResolution": "node",
      "forceConsistentCasingInFileNames": true,
      "noImplicitReturns": true,
      "noImplicitThis": false,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "suppressImplicitAnyIndexErrors": true,
      "noUnusedLocals": true
  },
}