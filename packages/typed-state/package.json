{
  "name": "@rematch/typed-state",
  "version": "2.1.0",
  "sideEffects": false,
  "description": "Rematch plugin for typing the state",
  "homepage": "https://rematchjs.org",
  "main": "dist/index.js",
  "module": "dist/typed-state.esm.js",
  "browser": "dist/typed-state.esm.js",
  "unpkg": "dist/typed-state.umd.production.min.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build --format cjs,esm,umd --tsconfig ./tsconfig.build.json",
    "test": "dts test",
    "lint": "dts lint",
    "clean": "rimraf dist"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "devDependencies": {
    "@rematch/core": "^2.1.0",
    "prop-types": "^15.7.2",
    "redux": "^4.0.5"
  },
  "peerDependencies": {
    "@rematch/core": "^2.0.0",
    "prop-types": "^15.7.2",
    "redux": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "author": "Shawn McKay <shawn.j.mckay@gmail.com> (https://github.com/shmck)",
  "keywords": [
    "@rematch",
    "rematch",
    "plugin",
    "updated"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rematch/rematch.git"
  }
}
